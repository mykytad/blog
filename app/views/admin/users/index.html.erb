<% provide(:title, "Admin Users") %>
<div class="row">
	<div class="col-3" style="background: #fff;border: 1px solid;">
		<p>block for other admin pages</p>
	</div>
	<div class="col-9 admin-artile" style="border: 1px solid;">
		<h3>Admin Users</h3>
		<ul class = "list-group admin_list">
			<% @users.each do |user| %>
			<li class = "list-group-item adminArtile_list_item ">
				<%= link_to user.name, user, class:"admin_list_link" %>
			</li>
			<% end %>
		</ul>
	</div>
</div>
